@model IEnumerable<WeChatHelloWorld1.Models.User_MerchantInfo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("添加菜品", "../products/create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WeChatOpenID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MerchantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MerchantAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.WeChatOpenID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MerchantName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MerchantAddress)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PhoneNumber)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
        @Html.ActionLink("Details", "Details", new { id=item.ID }) |
        @Html.ActionLink("Delete", "Delete", new { id=item.ID })
    </td>
</tr>
}

</table>
