@model IEnumerable<WeChatHelloWorld1.Models.CustomerOrderMaster>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>我的订单</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerOrder.MerchantID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerOrder.CustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerOrder.CustomerPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerOrder.DeliveryAdress)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerOrder.MerchantID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerOrder.CustomerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerOrder.CustomerPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerOrder.DeliveryAdress)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
